# Copy this file to .env before running the stack: cp .env.example .env

# Backend (FastAPI Operations API)
DATAMAX_PROJECT_NAME="Data Factory Operations API"
DATAMAX_ENVIRONMENT=development
DATAMAX_API_V1_PREFIX=/api/v1
DATAMAX_ALLOW_ORIGINS=["http://localhost:5173"]
DATAMAX_POSTGRES_DSN=sqlite+aiosqlite:///./operations.db
DATAMAX_REDIS_URL=
DATAMAX_MOCK_MODE=false
DATAMAX_LOGGING__LEVEL=INFO
DATAMAX_LOGGING__STRUCTURED=false
DATAMAX_LOGGING__FORMAT="{time:YYYY-MM-DD HH:mm:ss} | {level: <8} | {message}"

# Frontend (Vite)
VITE_API_BASE_URL=http://localhost:8000/api/v1

# QA generation script (examples/scripts/generate_qa.py)
DASHSCOPE_API_KEY=your_dashscope_api_key
DASHSCOPE_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
QA_MODEL=your_qa_model
QA_INPUT_SOURCE=oss

# OBS Config
OBS_ENDPOINT=https://obs.cn-north-4.myhuaweicloud.com
OBS_ACCESS_KEY_ID=your_obs_access_key_id
OBS_ACCESS_KEY_SECRET=your_obs_access_key_secret
OBS_BUCKET_NAME=your_bucket_name
OBS_DOWNLOAD_DIR=obs_downloads
OBS_PREFIX=

# OSS Config
OSS_ENDPOINT=https://oss-cn-hangzhou.aliyuncs.com
OSS_ACCESS_KEY_ID=your_oss_access_key_id
OSS_ACCESS_KEY_SECRET=your_oss_access_key_secret
OSS_BUCKET_NAME=your_bucket_name
OSS_DOWNLOAD_DIR=oss_downloads
OSS_PREFIX=

# Persistence configuration with URL
QA_DB_PREFIX=POSTGRES
QA_DB_DSN=
# DSN or URL
DATABASE_URL=

# Or Persistence configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=1921
POSTGRES_USERNAME=ai
POSTGRES_PASSWORD=your_secure_password
POSTGRES_DB=sdc-ai
POSTGRES_SCHEMA=sdc_ai
POSTGRES_CONNECT=postgresql

# Metadata tags for saved runs
QA_SOURCE_KEY=datamax_corpus
QA_SOURCE_NAME=your_source_name
QA_OWNER_TEAM=your_team
QA_CREATED_BY=your_name
QA_RUN_NAME=your_run
QA_TRIGGER_TYPE=manual
QA_MODEL_PROVIDER=aliyun
QA_MODEL_VERSION=1.0

# Optional overrides
QA_QUESTION_NUMBER=50
QA_CHUNK_SIZE=3000
QA_CHUNK_OVERLAP=1500
QA_MAX_QPS=100

# Schedule Interval For Daemon, 0 for single run
QA_SCHEDULE_INTERVAL=0

# root dir
DATAMAX_ROOT=/mnt/f/datamax
